#!/bin/sh
#
# PX4 FMUv4pro specific board sensors init
#------------------------------------------------------------------------------

# Internal SPI bus ICM-20608-G
mpu6000 -R 2 -T 20608 start

# Internal SPI bus ICM-20602
mpu6000 -R 2 -T 20602 start

# Internal SPI bus mpu9250
mpu9250 -R 2 start

# Internal SPI bus
lis3mdl -R 0 start

# Possible external compasses
hmc5883 -C -T -X start
qmc5883 -X start

# RM3100
rm3100 start
